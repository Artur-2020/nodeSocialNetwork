   <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
   <link rel="stylesheet" href="https://cdn.lineicons.com/2.0/LineIcons.css">
</head>
<body>
    <!-- navbar start -->
  <header>
   <div class="containerHead">
     <nav class="navbar navbar-expand-lg navbar-light my-navbar">
       <div class="container">
         <a class="navbar-brand" href="#">
           <img src="image/logo.png" alt="">
         </a>

          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
 
       <div class="collapse navbar-collapse" id="navbarSupportedContent">
         <ul class="navbar-nav ml-auto">
           <li class="nav-item">
             <a class="nav-link" href="#"> <i class="lni lni-home   linkIcon"></i>Home</a>
           </li>
           <li class="nav-item">
             <a class="nav-link" href="#"> <i class="lni lni-game linkIcon"></i> Games</a>
           </li>
           <li class="nav-item">
             <a class="nav-link" href="#"> <i class="lni lni-alarm linkIcon"></i>Notifications</a>
           </li>
           <li class="nav-item">
             <a class="nav-link" href="#">  <i class="lni lni-users linkIcon"></i>Friend Requests</a>
           </li>
           <li class="nav-item">
             <a class="nav-link " href="#"> <i class="lni lni-envelope linkIcon"></i>Messages</a>
           </li>
           <li class="nav-item">
            <input type="text" id="searchInp" placeholder="Search">
           </li>
           <li class="nav-item dropdown">
             <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
               
             </a>
             <div class="dropdown-menu" aria-labelledby="navbarDropdown">
               <a class="dropdown-item" href="profile/edit"> <i class="lni lni-cog"></i> Edit</a>
               <a class="dropdown-item" href="/login"> <i class="lni lni-exit"></i>Log Out</a>
               <div class="dropdown-divider"></div>
             </div>
           </li>
         </ul>
       </div>
       </div>
     </nav>
   </div>
 </header>
 <!-- navbar end -->

  <div id="main">
      <a href="/profile" class="backLink">  Back To Profile</a>
   <div id="addPhoto">
      <form method="post" action="/addPhoto"  enctype="multipart/form-data"> 
         <h2>Add Photo</h2>
         <input type="file" name="photo"> 	
         <button>Add Photo</button>   	
      </form>
   </div>
   <div id="photos">
      <h3 class="photosHead">Your Photos</h3>
      <div id="allPhotos">
    
         <% photos.forEach((i)=>{ %>
            <div class="image">
               <img src="<%=i.name%>">
               
               <button id='del' data-id=<%=i.id%>>Delete</button>
               <button id='set' data-id="<%=i.id%>">Set a Avatar</button>
               
   
            </div>
         <%})%>
      </div>
   </div>
   
   
  </div>
</body>
   <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.2/axios.min.js"></script>
   <script src="js/photos.js"></script>
</html>